<div class="container-fluid">
  <div class="row">
    <div class="col-md-4">
      <div class="panel panel-default">
        <div class="panel-body">
          <h4>Places index</h4>
        </div>
      </div>

      {{#each place in model}}
        <div class="panel panel-default">
          <div class="panel-body">
            <h4>{{place.name}}</h4>
            <h5>description:</h5>
            <p>{{place.description}}</p>
          </div>
        </div>
      {{/each}}

    </div>
    <div class="col-md-offset-4 col-md-4">
      {{outlet}}
    </div>
  </div>
</div>
